{
  "name": "@ra-aid/common",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc && postcss src/styles/global.css -o dist/styles/global.css",
    "dev": "tsc --watch",
    "watch:css": "postcss src/styles/global.css -o dist/styles/global.css --watch",
    "watch": "concurrently \"npm run dev\" \"npm run watch:css\"",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.370.0",
    "react-markdown": "^10.2.0",
    "react-syntax-highlighter": "^15.6.2",
    "remark-gfm": "^4.0.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.8",
    "zod": "^3.25.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.17",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.3",
    "postcss": "^8.4.36",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
